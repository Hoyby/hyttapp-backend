{
  "name": "hyttapp-backend",
  "version": "1.0.0",
  "description": "Cabin planner backend",
  "main": "index.js",
  "repository": "https://github.com/Hoyby/hyttapp-backend.git",
  "author": "Ole Alexander HÃ¸yby <alex.hoeyby@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.8.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.3.0",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.10",
    "ts-node": "^10.0.0"
  },
  "scripts": {
    "start": "node build/index.js",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "swagger": "tsoa spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
